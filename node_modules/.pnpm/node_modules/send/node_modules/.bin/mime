#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules/.pnpm/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/node_modules:/mnt/c/Users/mbous/OneDrive/node_modules:/mnt/c/Users/mbous/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/tp/js/ControllerOBS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules/.pnpm/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/ControllerOBS/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/JS/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/Tp/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/node_modules:/mnt/c/Users/mbous/OneDrive/node_modules:/mnt/c/Users/mbous/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mbous/OneDrive/Desktop/tp/js/ControllerOBS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
fi
